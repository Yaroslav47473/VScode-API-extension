{
  "name": "rest-api-tester",
  "displayName": "REST API Tester",
  "description": "REST API Testing VSCode Extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rest-api-tester.newRequest"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rest-api-tester.newRequest",
        "category": "APP Test",
        "title": "New Request"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "RESTAPITester",
          "title": "REST API Tester",
          "icon": "icons/httpIcon.svg"
        }
      ]
    },
    "views": {
      "RESTAPITester": [
        {
          "id": "REST API Tester",
          "name": ""
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "REST API Tester",
        "contents": "To start sending HTTP requests, press the 'New Request' button below. \n[New Request](command:rest-api-tester.newRequest)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "lint": "eslint --fix --ext .js,.jsx .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/node": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@vscode/test-electron": "^2.1.3",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.5",
    "buffer": "^6.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "file-loader": "^6.2.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "style-loader": "^3.3.1",
    "url": "^0.11.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.4.5",
    "axios": "^0.27.2",
    "formdata-node": "^4.3.3",
    "path-browserify": "^1.0.1",
    "postman-collection": "^4.1.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-spinners": "^0.13.3",
    "styled-components": "^5.3.5",
    "zustand": "^4.0.0-rc.1"
  }
}
